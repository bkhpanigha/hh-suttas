{
  "thag1.21:0.1": "Verses of the Senior Monks 1.21 ",
  "thag1.21:0.2": "The Book of the Ones ",
  "thag1.21:0.3": "Chapter Three ",
  "thag1.21:0.4": "Nigrodha ",
  "thag1.21:1.1": "I am not afraid of fear; ",
  "thag1.21:1.2": "our teacher is expert in the Deathless. ",
  "thag1.21:1.3": "The bhikkhus tread the path ",
  "thag1.21:1.4": "where fear cannot land. "
}